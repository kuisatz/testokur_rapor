WITH regi AS (
select row_number() over() as sirano1,  a.*  from  (
SELECT distinct
	std.first_name, std.surname,std.student_number,
	ser.success_count, ser.fail_count, ser.empty_count, ser.total_count, ser.total_net,ser.point,

	ls.name as ders ,
	cls.name as sinif,
 	cls.branch as sube,
	cls.class_level  ,
	ee.name as sinav,
	elb.name as kitapcik,
	ser.student_id,
	ser.class_id ,
	cast(ser.exam_date as date) as exam_date,
	rp.color_type,
	ee.id as exam_id,
	elb.question_count as question_count,
	sea.student_exam_result_id,
	sea.exam_lesson_booklet_id,
        CASE
	  WHEN   rp.class_level >0  THEN cast(cls.class_level as character varying(5))
	   ELSE     'HEPSİ'
	  END  as sinifx ,
	 CASE
	  WHEN  rp.branch_id >0  THEN cls.branch
	   ELSE     'HEPSİ'
	  END as subex,
	  ser.classroom_net_avg,



	  sea.lesson_classroom_score_avg,
	sea.lesson_class_level_score_avg, sea.lesson_classroom_net_avg, sea.lesson_class_level_net_avg,
     --  sea.lesson_classroom_order, sea.lesson_class_level_order,
 sea.lesson_score,
       sea.lesson_net_count, sea.lesson_success_percentage ,
       ls.sort

FROM student_exam_answers sea
inner join reports_params rp on rp.id = (select max(id) from reports_params )

inner join exam_lesson_booklets elb on elb.id = sea.exam_lesson_booklet_id
inner join lessons ls on ls.id = sea.lesson_id
inner join exam_lessons el on el.id = elb.exam_lesson_id
inner join exams ee on ee.id = el.exam_id
inner join student_exam_results ser on ser.id = sea.student_exam_result_id
inner join classes cls on cls.class_level= rp.class_level
inner join students std on std.id = ser.student_id and cls.id= std.class_id
inner join answerkeys answer on answer.exam_lesson_booklet_id = sea.exam_lesson_booklet_id and sea.question_number = answer.question_number and answer.deleted=false
  where
     ser.exam_id = 105 and --  rp.exam_id and
CASE
  WHEN  rp.branch_id >0  THEN   cls.id = rp.branch_id
   ELSE    1=1
  END
and
 CASE
  WHEN   rp.class_level>0  THEN   cls.class_level =  rp.class_level
   ELSE    1=1
  END

 --and sea.student_exam_result_id = 9498

 ) as a
  ),
answer_box AS (
Select answerb.* from answerkeys answerb
where answerb.exam_lesson_booklet_id  in (Select distinct rgx.exam_lesson_booklet_id from regi rgx    ) and answerb.deleted=false
),
student_answers AS (
Select stuanw.* from student_exam_answers stuanw
where stuanw.student_exam_result_id in (select distinct rgxx.student_exam_result_id from regi rgxx )

),
sinif_ortalamasi AS (
select sum(point) as sinif_ortalamasi_sum from  (
Select distinct  stuanwq.point ,stuanwq.id  from student_exam_results stuanwq
where stuanwq.id in (select distinct rgxx.student_exam_result_id from regi rgxx )
) as hhh
),
okul_ortalamasi AS (
select sum(point) as okul_ortalamasi_sum from (
Select distinct  stuanwqq.point ,stuanwqq.exam_id  from student_exam_results stuanwqq
where stuanwqq.exam_id  in (select distinct rgxx.exam_id   from regi rgxx )
) as rrtt
),





regi22 AS (
select   regi.* ,cc.corparation_name,
	ci.name as city,di.name as district,
	(select count(distinct ffx.point)+1 from student_exam_results ffx where ffx.student_id in (Select distinct sstt.id from students sstt where  sstt.class_id = regi.class_id ) and ffx.exam_id = regi.exam_id and  ffx.point > regi.point ) as sinif_sirasi,
        (select count(distinct ffx.point)+1 from student_exam_results ffx where ffx.exam_id = regi.exam_id and  ffx.point > regi.point ) as okul_sirasi ,
	(select max(sirano1) from regi ) as kayit_sayisi
from regi
inner join customers cc on cc.id = 1
inner join city ci on ci.id = cc.city_id
inner join districts di on di.id = cc.district_id
order by  sinif_sirasi , okul_sirasi , first_name, surname , sort
)



 select    row_number() over() as sirano,  * ,
	  cast(sinif_ortalamasi_sum / ogr_sayisi as numeric(18,3))  as sinif_ortalamasi ,
	  cast(okul_ortalamasi_sum / clslvl_ogrsayisi as numeric(18,3)) as okul_ortalamasi from (

 select       *, (select count(distinct student_id) from regi ) as ogr_sayisi ,
	(select count(distinct ser.student_id) FROM student_exam_answers sea
	inner join student_exam_results ser on ser.id = sea.student_exam_result_id
	inner join exams ee on ee.id = ser.exam_id and ee.id = 105 --  rp.exam_id
	inner join classes cls on cls.class_level= 5 -- rp.class_level
	inner join students std on std.id = ser.student_id and cls.id= std.class_id

	) as clslvl_ogrsayisi ,
	(select sinif_ortalamasi_sum from sinif_ortalamasi  ) as sinif_ortalamasi_sum ,
	(select okul_ortalamasi_sum from okul_ortalamasi  ) as okul_ortalamasi_sum
